{
  "name": "koishi-plugin-markdown",
  "description": "Markdown component support for Koishi",
  "version": "1.1.1",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "author": "Shigma <shigma10826@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koishijs/koishi-plugin-markdown.git"
  },
  "bugs": {
    "url": "https://github.com/koishijs/koishi-plugin-markdown/issues"
  },
  "homepage": "https://markdown.koishi.chat",
  "scripts": {
    "build": "atsc -b",
    "docs:dev": "vitepress dev docs --open",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "mocha --exit -r esbuild-register -r yml-register tests/*.spec.ts",
    "test:text": "shx rm -rf coverage && c8 -r text yarn test",
    "test:json": "shx rm -rf coverage && c8 -r json yarn test",
    "test:html": "shx rm -rf coverage && c8 -r html yarn test"
  },
  "koishi": {
    "browser": true,
    "category": "tool",
    "description": {
      "en": "Markdown component support",
      "zh": "Markdown 组件支持"
    }
  },
  "keywords": [
    "chatbot",
    "koishi",
    "plugin",
    "markdown",
    "component",
    "element",
    "render",
    "parser"
  ],
  "peerDependencies": {
    "koishi": "^4.10.5"
  },
  "devDependencies": {
    "@koishijs/plugin-mock": "^2.3.2",
    "@koishijs/vitepress": "^1.5.3",
    "@types/chai": "^4.3.4",
    "@types/marked": "^4.0.8",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.11.18",
    "atsc": "^1.2.2",
    "c8": "^7.12.0",
    "chai": "^4.3.7",
    "esbuild": "^0.14.54",
    "esbuild-register": "^3.4.2",
    "koishi": "^4.11.2",
    "mocha": "^9.2.2",
    "sass": "^1.57.1",
    "shx": "^0.3.4",
    "typescript": "^4.9.4",
    "vitepress": "1.0.0-alpha.26"
  },
  "dependencies": {
    "marked": "^4.2.12"
  }
}
